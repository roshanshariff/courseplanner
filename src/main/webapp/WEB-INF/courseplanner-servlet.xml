<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="simpleServletHandlerAdapter"
		class="org.springframework.web.servlet.handler.SimpleServletHandlerAdapter">
	</bean>

	<bean id="simpleServletPostProcessor"
		class="org.springframework.web.servlet.handler.SimpleServletPostProcessor">
	</bean>

	<bean id="courseDataService" name="/courseplanner/coursedata"
		class="ca.ualberta.cs.courseplanner.server.CourseDataServiceImpl">
		<property name="courseData" ref="courseData"/>
	</bean>
	
</beans>
